{
  "manifest_version": 3,
  "name": "3pl Mods",
  "version": "0.1.18",
  "update_url": "https://wms.bful.co/ext/update.xml",
  "description": "The BFF Chrome Extension",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://global.secure-wms.com/*"],
      "js": ["borderify.js"]
    },
    {
      "matches": ["*://global.secure-wms.com/*"],
      "js": ["wms.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "demoModule.js","noErrorDisable.js"],
      "matches": ["*://global.secure-wms.com/*"]
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "action": {
    "default_title": "Drink Water Event",
    "default_popup": "options.html"
  },
  "permissions": [
    "storage",
    "gcm"
  ]
}